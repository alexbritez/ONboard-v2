   <scene id="21" type="End_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
      <multiple_choice>
         <multiple>
            <question>The vertical axis on the graph is the x-axis.</question>
            <answers>
               <answer>True</answer>
               <answer correct="true">False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>When graphing experimental data, the independent variable is the x-axis on the graph.</question>
            <answers>
               <answer correct="true">True</answer>
               <answer>False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>The intervals used for the numerical values on the x-axis and the y-axis do not need to be the same.</question>
            <answers>
               <answer correct="true">True</answer>
               <answer>False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>You must show every whole number along an axis.</question>
            <answers>
               <answer>True</answer>
               <answer correct="true">False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>Each data point on a graph is made up of the x-axis value and the corresponding y-axis value.</question>
            <answers>
               <answer correct="true">True</answer>
               <answer>False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

      </multiple_choice>
