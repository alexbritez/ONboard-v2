			<width>134</width>
			<popTitle></popTitle>
			<popInfo><![CDATA[<p>The Magna Carta was signed by the English monarch King John I in 1215.</p>]]></popInfo>
		</pButton>
		
		<pButton> 
			<Name>2</Name>   
			<audio enable="true" url="audio/euro_m3-2_s16_b.mp3"/>
