   <scene id="10" type="Activity_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
      <multiple_choice>
         <multiple>
            <question>A fluorine atom has 9 protons and 10 neutrons. What is the atomic number of fluorine?</question>
            <answers>
               <answer correct="true">9</answer>
			   <answer>18</answer>
               <answer>19</answer>
            </answers>
            <feedbackAudio>bio_m5-1_s10_FB</feedbackAudio>
         </multiple>
      </multiple_choice>
