   <scene id="10" type="Activity_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
      <multiple_choice>
         <multiple hasFigure="true">
            <question>How many electrons does each atom need to become stable?</question>
            <answers>
               <answer>The carbon atom needs 8 and the oxygen atoms each need 4.</answer>
               <answer correct="true">The carbon atom needs 4 and the oxygen atoms each need 2.</answer>
            </answers>
            <figure>bio_m5-2_s10_view.jpg</figure>
            <feedbackAudio>bio_m5-2_s10_FB</feedbackAudio>
         </multiple>
      </multiple_choice>
