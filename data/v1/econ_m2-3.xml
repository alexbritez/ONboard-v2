<shell title="Identify a Point on a Graph">
  <scene id="1" type="1_Module_Intro">
    <video>econ_m2-3_s1</video>
    <credits>(bkgd)Maria Toutoudaki/Photodisc/Getty Images, (others)Don Bishop/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="2" type="7_Activity_Drag_Drop">
    <matchingInstructions>Match the items on the left to the correct areas on the right.</matchingInstructions>
    <matching type="dynamic" format="letter" deductionFactor="2" maxAttempts="3" hasPoints="true" points="6">
      <matches title="">
        <match id="1"></match><!--labels omitted on purpose - they just wanted letters.-->
		<match id="2"></match>
		<match id="3"></match>
		<match id="4"></match>
		<match id="5"></match>
      </matches>
      <drops title="" children-styles="margin-bottom: 0.1em;">
        <drop correct="1" x="517" y="279"></drop>
		<drop correct="2" x="453" y="239"></drop>
		<drop correct="3" x="389" y="199"></drop>
		<drop correct="4" x="325" y="159"></drop>
		<drop correct="5" x="261" y="119"></drop>
      </drops>
      <image>econ_m2-3_s2.jpg</image>
    </matching>
    <instructions>When you are done, check your results by clicking 'Check Answers.'</instructions>
    <audio>econ_m2-3_s2</audio>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="3" type="2_Txt_Img_Animation">
    <video>econ_m2-3_s3</video>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="4" type="Activity_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
    <multiple_choice>
      <multiple hasFigure="true">
        <question>According to this graph, how many corndogs will be supplied at a price of $3?</question>
        <answers>
          <answer>100</answer>
		  <answer>150</answer>
          <answer correct="true">200</answer>
		  <answer>250</answer>
		  <answer>300</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s4_view.jpg</figure>
        <feedbackAudio>econ_m2-3_s4_FB</feedbackAudio>
      </multiple>
    </multiple_choice>
    <audio>econ_m2-3_s4</audio>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="5" type="Activity_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
    <multiple_choice>
      <multiple hasFigure="true">
        <question>If suppliers are currently producing 350 corndogs, what is the price of corndogs?</question>
        <answers>
          <answer>$3</answer>
		  <answer>$4</answer>
		  <answer>$5</answer>
          <answer correct="true">$6</answer>
		  <answer>$7</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s4_view.jpg</figure>
        <feedbackAudio>econ_m2-3_s5_FB</feedbackAudio>
      </multiple>
    </multiple_choice>
    <audio>econ_m2-3_s5</audio>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="6" type="Activity_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
    <multiple_choice>
      <multiple hasFigure="true">
        <question>How many corndogs will be supplied at a price of $5?</question>
        <answers>
          <answer>100</answer>
		  <answer>150</answer>
		  <answer>200</answer>
		  <answer>250</answer>
          <answer correct="true">300</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s4_view.jpg</figure>
        <feedbackAudio>econ_m2-3_s6_FB</feedbackAudio>
      </multiple>
    </multiple_choice>
    <audio>econ_m2-3_s6</audio>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>

  <scene id="7" type="End_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
    <multiple_choice>
      <multiple hasFigure="true">
        <question>This graph shows the relationship between what &lt;br /&gt;two variables?</question>
        <answers>
          <answer>Price of MP3 players and Quantity Supplied of MP3 players</answer>
		  <answer>Quantity Supplied of MP3 players and Quantity Demanded of MP3 players</answer>
		  <answer>Price of MP3 players and Value of MP3 players</answer>
          <answer correct="true">Price of MP3 players and Quantity Demanded of MP3 players</answer>
		  <answer>Price to both produce and purchase MP3 players</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s7_view.jpg</figure>
        <feedbackAudio></feedbackAudio>
      </multiple>
	  <multiple hasFigure="true">
        <question>According to this graph, how many MP3 players will be demanded at a price of $200?</question>
        <answers>
          <answer>400</answer>
          <answer correct="true">700</answer>
		  <answer>800</answer>
		  <answer>900</answer>
		  <answer>0</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s7_view.jpg</figure>
        <feedbackAudio></feedbackAudio>
      </multiple>
	  <multiple hasFigure="true">
        <question>If consumers are demanding 300 MP3 players, what is the price of MP3 players?</question>
        <answers>
          <answer>$500</answer>
          <answer correct="true">$400</answer>
		  <answer>$300</answer>
		  <answer>$200</answer>
		  <answer>$100</answer>
        </answers>
        <figure verticalAlign="center">econ_m2-3_s7_view.jpg</figure>
        <feedbackAudio></feedbackAudio>
      </multiple>
	  <multiple>
        <question>Quantity supplied is always labeled on the x-axis.</question>
        <answers>
          <answer correct="true">True</answer>
          <answer>False</answer>
        </answers>
        <feedbackAudio></feedbackAudio>
      </multiple>
	  <multiple>
        <question>Price is always labeled on the horizontal axis.</question>
        <answers>
          <answer>True</answer>
          <answer correct="true">False</answer>
        </answers>
        <feedbackAudio></feedbackAudio>
      </multiple>
    </multiple_choice>
    <audio>econ_m2-3_s7</audio>
    <credits>(tr)Don Bishop/Photodisc/Getty Images, (bkgd)Maria Toutoudaki/Photodisc/Getty Images.</credits>
  </scene>
</shell>
