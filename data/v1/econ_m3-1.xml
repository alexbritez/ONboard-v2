  <scene id="3" type="6_Activity_Interactivity">
    <content>
      <header>Converting Percentages</header>
      <instruction>
        <![CDATA[
          <p>
            Percentage
            <br />
            25%
          </p>
          <p>
            Fraction
            <br />
            25% = 25/100 = 1/4
          </p>
          <p>
            Decimal
            <br />
            25% = 0.25
          </p>
        ]]>
      </instruction>
    </content>
