   <scene id="30" type="End_Multiple_Choice">
    <instruction>Click on the correct answer.</instruction>
      <multiple_choice>
         <multiple>
            <question>Use a bar graph to show how the whole of something is divided into parts.</question>
            <answers>
               <answer>True</answer>
			   <answer correct="true">False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>Use a double-bar graph to show more than one variable for each condition.</question>
            <answers>
               <answer correct="true">True</answer>
			   <answer>False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>Use a circle graph to display data that are continuous.</question>
            <answers>
               <answer>True</answer>
			   <answer correct="true">False</answer>
			</answers>
			   <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>Use a line graph to display and analyze data that are discontinuous.</question>
            <answers>
               <answer>True</answer>
			   <answer correct="true">False</answer>

            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

		 <multiple>
            <question>Multiple-line graphs are used to display two or more sets of continuous data.</question>
            <answers>
                <answer correct="true">True</answer>
			   <answer>False</answer>
            </answers>
            <figure></figure>
            <feedbackAudio></feedbackAudio>
         </multiple>

      </multiple_choice>
